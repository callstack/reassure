{
  "name": "@callstack/reassure-logger",
  "version": "1.0.0-rc.3",
  "description": "Logger for Reassure project",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
