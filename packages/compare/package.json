    "@types/react": "^19.0.0",
    "babel-jest": "^30.0.2",
    "del-cli": "^5.1.0",
    "jest": "^30.0.2",
    "prettier": "^2.8.8",
    "react-native-builder-bob": "^0.24.0",
    "typescript": "^5.8.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
