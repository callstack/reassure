{
  "name": "@reassure/reassure-cli",
  "version": "0.2.0",
  "description": "Performance testing companion for React and React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "bin": {
    "reassure": "lib/commonjs/bin.js"
  },
  "files": [
    "src",
    "lib",
    "!lib/typescript/examples",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "bob build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/callstack/reassure",
  "author": "Maciej Jastrzębski <mdjastrzebski@gmail.com> (https://github.com/mdjastrzebski)",
  "contributors": [
    "Jakub Bujko <jakub.bujko@callstack.com> (https://github.com/Xiltyn)",
    "Tomasz Krzyżowski <tomasz.krzyzowski@callstack.com> (https://github.com/TMaszko)",
    "Michał Pierzchała <thymikee@gmail.com> (https://github.com/thymikee)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/callstack/reassure/issues"
  },
  "homepage": "https://github.com/callstack/reassure#readme",
  "dependencies": {
    "@reassure/reassure-compare": "0.1.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@callstack/eslint-config": "^12.0.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.39",
    "@types/react-native": "0.66.16",
    "@types/yargs": "^17.0.10",
    "babel-jest": "^27.5.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^6.0.0",
    "jest": "^27.4.3",
    "prettier": "^2.0.5",
    "react-native-builder-bob": "^0.18.0",
    "typescript": "^4.5.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
